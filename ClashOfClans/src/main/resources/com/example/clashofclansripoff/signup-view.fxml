<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="444.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clashofclansripoff.controller.SignupController">
   <children>
      <ImageView fitHeight="444.0" fitWidth="909.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@CoC.png" />
         </image>
      </ImageView>
      <TextField fx:id="pass_field" layoutX="349.0" layoutY="240.0" promptText="Password">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
      </TextField>
      <TextField fx:id="email_field" layoutX="349.0" layoutY="136.0" promptText="Email">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
      </TextField>
      <Slider fx:id="age_slider" blockIncrement="1.0" layoutX="349.0" layoutY="290.0" majorTickUnit="1.0" min="1.0" minorTickCount="1" onMouseReleased="#age_changed" prefHeight="23.0" prefWidth="211.0" value="1.0" />
      <Text fill="RED" layoutX="29.0" layoutY="308.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Age:(You must be older than 13)" wrappingWidth="290.936767578125">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
      </Text>
      <Text fx:id="age" fill="BLUE" layoutX="590.0" layoutY="308.0" strokeType="OUTSIDE" strokeWidth="0.0" text="1" wrappingWidth="27.73675537109375">
         <font>
            <Font size="17.0" />
         </font>
      </Text>
      <Button layoutX="560.0" layoutY="328.0" mnemonicParsing="false" onMouseClicked="#submit" prefHeight="38.0" prefWidth="88.0" text="Submit">
         <font>
            <Font name="Arial Black" size="17.0" />
         </font>
      </Button>
      <Button fx:id="back_btn1" layoutX="261.0" layoutY="328.0" mnemonicParsing="false" onMouseClicked="#exit" prefHeight="38.0" prefWidth="88.0" text="Back">
         <font>
            <Font name="Arial Black" size="17.0" />
         </font>
      </Button>
      <TextField fx:id="username_field" layoutX="349.0" layoutY="186.0" promptText="Username">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
